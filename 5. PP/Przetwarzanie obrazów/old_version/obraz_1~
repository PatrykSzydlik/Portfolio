KOD:
*****************************************

if(obraz->wariant[1]=='3')
  {
    printf("\n Czytaj P3 %d %d \n",obraz->wymx,obraz->wymy);
    for (i=0;i<obraz->wymy;i++)
      {
        printf("dupa %d %d\n",i,obraz->wymy);
        for (j=0;j<(KOLOR*(obraz->wymx));j++)
          {
            if ((fscanf(plik_we,"%d",&obraz->piksele[i][j]))!=1)
              {
                printf("TU");
                fprintf(stderr,"Plik: file2.c   Funkcja: czytaj  Linia: 96\n");
                return B_BLEDNAWARTOSCI;
              }
            printf("TAM %d %d %d\n",i,j,obraz->piksele[i][j]);
          }
        printf("TUTAJ\n");
      }
    printf("MOZE TUTAJ");
  }
 printf("Tutaj musi");


